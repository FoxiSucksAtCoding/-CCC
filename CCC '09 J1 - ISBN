import java.io.*;
import java.util.*;
 

public class Program
{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int onethreesum = a*1 + b*3 + c*1 + 91;
        
        System.out.println("The 1-3-sum is " + onethreesum);
        
        
	}
}
